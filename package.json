{
  "name": "bibliotekdk-next-widgets",
  "version": "0.0.1",
  "description": "building embeddable bibliotekdk widgets with react",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack-cli --optimize-minimize --display-modules",
    "dev": "start-storybook -p 3000",
    "test": "jest",
    "test-update-snapshots": "jest --updateSnapshot",
    "deploy": "npm run build && gh-pages -d dist",
    "storybook": "API_URI=http://localhost:3001/api/v1 start-storybook -p 3000",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook --quiet",
    "lint": "eslint src/",
    "cy:run": "npx cypress@latest run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test-helpers/"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Frontend @ DBC",
  "license": "GNU",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-syntax-import-meta": "7.7.4",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-preset-airbnb": "4.4.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.1",
    "cssimportant-loader": "0.4.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss-increase-specificity": "0.6.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "8.0.0",
    "style-loader": "1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "knex": "^0.20.9",
    "lodash": "^4.17.15",
    "pg": "^7.18.1",
    "preact": "^10.2.1",
    "prop-types": "^15.7.2",
    "unfetch": "^4.1.0"
  }
}
