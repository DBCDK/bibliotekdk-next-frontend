{
  "name": "bibliotekdk-next-frontend",
  "version": "0.0.1",
  "description": "The frontend for bibliotek.dk",
  "main": "index.js",
  "scripts": {
    "dev:next": "next",
    "build:next": "next build",
    "start:next": "next start",
    "dev:storybook": "start-storybook -p \"${STORYBOOK_PORT:-4000}\"",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook --quiet",
    "start:storybook": "[ \"$ENABLE_STORYBOOK\" = \"1\" ] && http-server -p \"${STORYBOOK_PORT:-4000}\" dist/storybook --silent || exit 0",
    "dev": "run-p dev:storybook dev:next",
    "start": "run-p start:storybook start:next",
    "lint": "eslint src/",
    "cy:run": "npx cypress@latest run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "author": "Frontend @ DBC",
  "license": "GNU",
  "devDependencies": {
    "@storybook/react": "^6.0.16",
    "babel-eslint": "^10.1.0",
    "css-loader": "^3.4.1",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "style-loader": "^1.1.2"
  },
  "dependencies": {
    "http-server": "^0.12.3",
    "next": "^9.5.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "unfetch": "^4.1.0"
  }
}
