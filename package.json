{
  "name": "bibliotekdk-next-frontend",
  "version": "0.0.1",
  "description": "The frontend for bibliotek.dk",
  "main": "index.js",
  "scripts": {
    "dev:next": "next -p ${PORT:-3000}",
    "build:next": "next build",
    "start:next": "next start -p ${PORT:-3000}",
    "dev:storybook": "STORYBOOK_ACTIVE=1 start-storybook -s ./public -p \"${STORYBOOK_PORT:-4000}\"",
    "build:storybook": "STORYBOOK_ACTIVE=1 build-storybook -c .storybook -o dist/storybook --quiet && cp -r public/* dist/storybook/",
    "start:storybook": "[ \"$ENABLE_STORYBOOK\" = \"1\" ] && http-server -p \"${STORYBOOK_PORT:-4000}\" dist/storybook --silent || exit 0",
    "dev": "run-p dev:storybook dev:next",
    "start": "run-p start:storybook start:next",
    "lint": "eslint src/",
    "cy:run": "npx cypress@latest run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e",
    "prettier": "prettier --write 'src/**/*.js'",
    "test": "jest src/"
  },
  "author": "Frontend @ DBC",
  "license": "GNU",
  "devDependencies": {
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/react": "^6.0.16",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "css-loader": "^3.4.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.0",
    "prettier": "^2.0.5",
    "storybook-addon-next-router": "^2.0.0",
    "style-loader": "^1.1.2"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "bootstrap": "^4.5.0",
    "http-server": "^0.12.3",
    "isbot": "^3.0.13",
    "isomorphic-unfetch": "^3.0.0",
    "lazysizes": "^5.2.2",
    "next": "^10.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.4",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-id-swiper": "^4.0.0",
    "swiper": "^6.3.5",
    "unfetch": "^4.1.0"
  }
}
