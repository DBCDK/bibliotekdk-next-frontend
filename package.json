{
  "name": "bibliotekdk-next-frontend",
  "version": "0.0.1",
  "description": "The frontend for bibliotek.dk",
  "main": "index.js",
  "scripts": {
    "dev:next": "next -p ${PORT:-3000}",
    "build:next": "next build",
    "start:next": "next start -p ${PORT:-3000}",
    "dev:storybook": "STORYBOOK_ACTIVE=1 start-storybook -s ./public -p \"${STORYBOOK_PORT:-4000}\"",
    "build:storybook": "STORYBOOK_ACTIVE=1 build-storybook -c .storybook -o dist/storybook --quiet && cp -r public/* dist/storybook/",
    "start:storybook": "[ \"$ENABLE_STORYBOOK\" = \"1\" ] && http-server -p \"${STORYBOOK_PORT:-4000}\" dist/storybook --silent || exit 0",
    "dev": "run-p dev:storybook dev:next",
    "start": "run-p start:storybook start:next",
    "lint": "eslint src/",
    "cy:run": "npx cypress@9 run --project ./e2e",
    "cy:open": "npx cypress@9 open --project ./e2e",
    "prettier": "prettier --write 'src/**/*.js'",
    "test": "jest src/"
  },
  "author": "Frontend @ DBC",
  "license": "GNU",
  "devDependencies": {
    "@graphql-tools/mock": "^8.6.1",
    "@graphql-tools/schema": "^8.3.3",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/react": "^6.4.9",
    "@svgr/webpack": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "css-loader": "^3.4.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql": "^15.8.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.2",
    "sass-loader": "^10",
    "storybook-addon-next-router": "^3.1.1",
    "style-loader": "^1.1.2"
  },
  "dependencies": {
    "@dbcdk/login-nextjs": "^3.2.2",
    "@rooks/use-mutation-observer": "^4.11.0",
    "autosuggest-highlight": "^3.1.1",
    "bootstrap": "^4.5.0",
    "dbc-node-logger": "^2.0.9",
    "http-server": "^0.12.3",
    "identity-obj-proxy": "^3.0.0",
    "isbot": "^3.0.13",
